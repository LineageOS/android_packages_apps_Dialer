<resources>
  <!-- Button shown during a phone call to upgrade to video.
     [CHAR LIMIT=12] -->
  <string name="incall_label_videocall">Gairm video</string>
  <!-- Button shown during a phone call to upgrade to Real-time Text.
     [CHAR LIMIT=12] -->
  <string name="incall_label_rttcall">Gairm teacsa fìor-ama (RTT)</string>
  <!-- Button shown during a phone call to put the call on hold.
     [CHAR LIMIT=12] -->
  <string name="incall_label_hold">Cùm</string>
  <!-- Button shown during a phone call to add a new phone call.
     [CHAR LIMIT=12] -->
  <string name="incall_label_add_call">Cuir gairm ris</string>
  <!-- Button shown during a phone call to mute the microphone.
     [CHAR LIMIT=12] -->
  <string name="incall_label_mute">Mùch</string>
  <!-- Button shown during a phone call to end the call.
     [CHAR LIMIT=12] -->
  <string name="incall_label_end_call">Cuir crìoch air a’ ghairm</string>
  <!-- Button shown during a phone call to show the dialpad.
     [CHAR LIMIT=12] -->
  <string name="incall_label_dialpad">Pada-iuchraichean</string>
  <!-- Talkback text for speaker button status. [CHAR LIMIT=12] -->
  <string name="incall_talkback_speaker_on">, tha e air</string>
  <!-- Talkback text for speaker button status. [CHAR LIMIT=12] -->
  <string name="incall_talkback_speaker_off">, tha e dheth</string>
  <!-- Button shown during a phone to merge two ongoing calls.
     [CHAR LIMIT=12] -->
  <string name="incall_label_merge">Co-aonaich</string>
  <!-- Button shown during a phone to show the manage conference call screen.
     [CHAR LIMIT=12] -->
  <string name="incall_label_manage">Stiùirich</string>
  <string name="a11y_description_incall_label_manage_content">Stiùirich na gairmearan</string>
  <!-- Button shown during a phone to swap from the foreground call to the background call.
     [CHAR LIMIT=12] -->
  <string name="incall_label_swap">Suaip</string>
  <!-- Button shown during a call with a multi-SIM device to hang up and call with the other SIM
     instead. [CHAR LIMIT=12] -->
  <string name="incall_label_swap_sim">Atharraich an SIM</string>
  <!-- Used to inform the user that the note associated with an outgoing call has been sent.
       [CHAR LIMIT=32] -->
  <string name="incall_note_sent">Chaidh an nòta a chur</string>
</resources>
